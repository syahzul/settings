{
  "name": "edvinaskrucas/settings",
  "keywords": ["settings", "persistent settings", "laravel"],
  "description": "Persistent settings package for Laravel framework.",
  "authors": [
    {
      "name": "Edvinas Kruƒças",
      "email": "edv.krucas@gmail.com"
    }
  ],

  "extra": {
    "laravel": {
      "providers": [
        "Krucas\\Settings\\Providers\\SettingsServiceProvider"
      ],
      "aliases": {
        "Settings": "Krucas\\Settings\\Facades\\Settings"
      }
    }
  },

  "license": "MIT",
  "require": {
    "php" : "^7.2",
    "illuminate/support": "~6.0",
    "illuminate/console": "~6.0",
    "illuminate/database": "~6.0",
    "illuminate/filesystem": "~6.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "8"
  },
  "autoload": {
    "psr-0": {
      "Krucas\\Settings\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev"
}
